{
	"filter": "trace",
	"loggers": [
		{
			"channel": "console",
			"severity": "trace",
			"format": "%H:%M:%S.%i %I %q: %t",
			"bufsize": 1048576
		},
		{
			"channel": "file",
			"severity": "trace,debug,info,warning,error,fatal",
            "format": "%y-%m-%d %H:%M:%S %I %q: %t",
			"path1": "monik.log",
			"path2": "monik2.log",
			"logsize": 0,
			"bufsize": 0
		},
		{
			"channel": "monik",
			"severity": "trace,debug,info,warning,error,fatal",
            "format": "%y-%m-%d %H:%M:%S %I %q: %t",
			"params": {
				"host": "",
				"port": 0,
				"user": "",
				"password": "",
				"exchange": "",
				"source": "",
				"instance": "",
				"tags": "",
				"durable": true,
				"retrytimeout": 10
			},
			"bufsize": 0
		},
		{
			"channel": "keepalive",
			"severity": "trace",
			"format": "%t",
			"params": {
				"host": "",
				"port": 0,
				"user": "",
				"password": "",
				"exchange": "",
				"source": "",
				"instance": "",
				"tags": "",
				"durable": true,
				"period": 60,
				"retrytimeout": 10
			},
			"bufsize": 0
		}
	]
}